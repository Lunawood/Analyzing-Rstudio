# x에 1할당하기
x <- 1
print(x)

# String형 할당하기
msg<-"hello"
msg

# vector 및 matrix 생성
x <- c(0.5, 0.6)
x

x <- c("abc", "def", "zzz")
x

m <- matrix(nrow = 2, ncol = 3)
m

m <- matrix(1:6, nrow = 2, ncol = 3)
m

# list 생성
x <- list(1, "abc", TRUE, 1+4i)
x

# Dataframe 생성
x <- data.frame(id= 1:4, name=c("kim", "lee", "park", "choi"))
x
